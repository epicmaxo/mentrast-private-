{
    "version": 2,
    "builds": [
        {
            "src": "invite-service/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "invite-service/public/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/invite-service/server.js"
        }
    ]
}